  "dependencies": {
    "alt": "~0.17.1",
    "async": "^1.4.2",
    "babel": "^5.8.23",
    "body-parser": "^1.14.1",
    "bootstrap": "~3.3.5",
    "cloudant": "^1.3.0",
    "compression": "^1.6.0",
