  "main": "server.js",
  "name": "plant",
  "pre-commit": {
    "colors": true,
    "run": [
      "dependency-update",
      "csslint",
      "test"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/plant.git"
  },
  "scripts": {
    "build": "TARGET=build webpack --bail",
    "check-coverage": "node_modules/.bin/istanbul check-coverage --statements 10 --branches 10 --functions 10 --lines 10 ./coverage/coverage.json",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --require test/setup.js test/**/*.js --recursive",
    "dependency-update": "node_modules/.bin/npm-update-outdated",
    "lint": "eslint --ext .js --ext .jsx . --max-warnings 0",
    "mocha": "DEBUG=plant:* node_modules/mocha/bin/_mocha test/**/*.js --recursive --require babel-core/register --require test/setup.js",
    "prepublish": "npm run build",
    "server": ". ./devops/run-server.sh",
    "start": "TARGET=dev webpack-dev-server --progress --colors --hot --inline --history-api-fallback --bail",
    "csslint": "stylelint app/stylesheets/*.css",
    "test": "npm run lint && npm run build && sh ./devops/run-test.sh"
  },
  "version": "0.0.1"
}
