    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "jquery": "~2.1.4",
    "jwt-simple": "^0.3.1",
    "lodash": "^3.10.0",
    "method-override": "^2.3.5",
    "morgan": "^1.6.1",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "react": "~0.13.3",
    "react-bootstrap": "~0.26.1",
    "react-router": "^0.13.4",
    "request": "^2.60.0",
    "serve-favicon": "^2.3.0",
    "yql": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^5.6.17",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.1",
    "css-loader": "0.x",
    "eslint": "*",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "node-libs-browser": "^0.5.2",
    "npm-update-outdated": "^0.1.6",
    "pre-commit": "^1.1.2",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "style-loader": "0.x",
    "url-loader": "0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.2.0"
  },
  "scripts": {
    "check-coverage": "node_modules/.bin/istanbul check-coverage --statements 10 --branches 10 --functions 10 --lines 10 ./coverage/coverage.json",
    "coverage": "node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test/**/*.js --recursive --require babel/register",
    "mocha": "node_modules/mocha/bin/_mocha test/**/*.js --recursive --require babel/register",
    "test": "DEBUG=plant:* npm run coverage",
    "build": "TARGET=build webpack",
    "start": "TARGET=dev webpack-dev-server --devtool eval-source --progress --colors --hot --inline --history-api-fallback",
    "prepublish": "npm run build",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx .",
    "dependency-update": "node_modules/.bin/npm-update-outdated"
  },
  "pre-commit": {
    "silent": false,
    "colors": true,
    "run": [
      "dependency-update",
      "lint",
      "test",
      "check-coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/plant.git"
  },
  "keywords": [
    "backyard",
    "orchard",
    "plant",
    "tracker"
  ],
