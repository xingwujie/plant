  "main": "index.js",
  "name": "plant",
  "pre-commit": {
    "colors": true,
    "run": [
      "dependency-update",
      "lint",
      "test",
      "check-coverage"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/plant.git"
  },
  "scripts": {
    "build": "TARGET=build webpack",
    "check-coverage": "node_modules/.bin/istanbul check-coverage --statements 10 --branches 10 --functions 10 --lines 10 ./coverage/coverage.json",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --require test/setup.js test/**/*.js --recursive",
    "dependency-update": "node_modules/.bin/npm-update-outdated",
    "lint": "eslint --ext .js --ext .jsx .",
    "mocha": "node_modules/mocha/bin/_mocha test/**/*.js --recursive --require babel-core/register",
    "prepublish": "npm run build",
    "prodstart": ". devops/setenv-prod.sh && NODE_ENV=production npm run build && NODE_ENV=production forever start -l forever.log -o out.log -e err.log -a index.js",
    "prodstop": "forever stop index.js",
    "start": "TARGET=dev webpack-dev-server --devtool eval-source --progress --colors --hot --inline --history-api-fallback",
    "test": "DEBUG=plant:* npm run coverage"
  },
  "version": "0.0.1"
}
